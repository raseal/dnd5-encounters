{
  "name": "thefoovault/sf-skeleton",
  "autoload": {
    "psr-4": {
      "Shared\\": "src/Shared",
      "EncounterAPI\\": "apps/EncounterAPI/src",
      "Encounter\\": "src/Encounter"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "extra": {
    "bin-dir": "apps/EncounterAPI/bin",
    "config-dir": "apps/EncounterAPI/config",
    "src-dir": "apps/EncounterAPI/src",
    "var-dir": "apps/EncounterAPI/var",
    "public-dir": "apps/EncounterAPI/public"
  },
  "require": {
    "symfony/flex": "^1.13",
    "symfony/framework-bundle": "^5.3",
    "symfony/dotenv": "^5.3",
    "symfony/yaml": "^5.3",
    "symfony/console": "^5.3",
    "symfony/runtime": "^5.3",
    "symfony/messenger": "^5.3",
    "lambdish/phunctional": "^2.1",
    "ramsey/uuid": "^4.2",
    "symfony/serializer": "^5.3",
    "symfony/property-access": "^5.3",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/orm": "^2.9",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "ext-mongodb": "*",
    "mongodb/mongodb": "^1.12"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "dg/bypass-finals": "^1.3",
    "phpstan/phpstan": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
